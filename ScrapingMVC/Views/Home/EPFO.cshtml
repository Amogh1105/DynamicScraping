
@{
    ViewBag.Title = "EPFO";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>EPFO</h2>


@if(ViewBag.Result)
{
    <fieldset>
        <legend>
            Results Obtained
        </legend>

        <div class="form-group">
            <label>Mean Confidence</label>
            <label>@ViewBag.mean</label>
        </div>
        <div class="form-group">
            <label>Result</label>
            <textarea class="form-control" readonly rows="10">@ViewBag.res</textarea>
        </div>
    </fieldset>
}
