<script type="text/javascript">
    function GetSummary() {
        var datevalue = $('#date').val();

        $.ajax({
            type: "GET",
            url: '@Url.Action("DrawReportsOnDashBoard", "Home")?datevalue=' + datevalue + '',

            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (result) {
                console.log(result); var row = "";

                var xx = 1;
                if (result.length != 0 && result != null) {
                    var html = '';
                    $.each(result, function (key, item) {
                        html += '<tr>';
                        html += '<td>' + item.cin + '</td>';
                        html += '<td>' + item.companyname + '</td>';
                     
                        html += '</tr>';
                    });

                    $('.tbody').html(html);
                    $('#thead').show();

                    $('#example1').show();
                } else {
                    row += "<tr><td style='color:red;'>No Records Found for this Date !!</td></tr>"
                    $("#tbody").html(row);
                    $('#thead').hide();

                }

            },
            failure: function (response) {
                alert(response.responseText);
            },
            error: function (response) {
                alert(response.responseText);
            }
        });

    }

</script>

